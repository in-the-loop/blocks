{
  "name": "@origen/blocks",
  "version": "1.0.1",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "sideEffect": false,
  "author": {
    "name": "Diego Stratta",
    "email": "strattadb@gmail.com",
    "url": "https://diegostratta.com"
  },
  "homepage": "https://blocks.origen.chat",
  "scripts": {
    "build": "yarn build-lib && yarn build-types",
    "build-lib": "cross-env NODE_ENV=production rollup --config rollup.config.js",
    "build-types": "tsc --emitDeclarationOnly",
    "prebuild": "yarn clean-dist",
    "storybook": "start-storybook --host 0.0.0.0 --port 5200 --config-dir .storybook --https --ssl-cert ./keys/cert.pem --ssl-key ./keys/key.pem",
    "build-storybook": "cross-env NODE_ENV=production build-storybook --config-dir .storybook --output-dir storybookDist",
    "prebuild-storybook": "yarn clean-storybookDist",
    "make-keys": "mkcert localhost 127.0.0.1 ::1 0.0.0.0",
    "premake-keys": "yarn clean-keys",
    "postmake-keys": "mkdirp keys && mv localhost+3.pem keys/cert.pem && mv localhost+3-key.pem keys/key.pem",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:watch": "yarn test --onlyChanged --watch",
    "test:ci": "yarn test --verbose --ci --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk=0.0.0.0:5100 --nolazy node_modules/.bin/jest --runInBand",
    "typecheck": "yarn tsc --noEmit",
    "lint": "yarn lint:check",
    "lint:check": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "clean-dist": "rimraf dist",
    "clean-storybookDist": "rimraf storybookDist",
    "clean-keys": "rimraf keys/cert.pem keys/key.pem",
    "upload-coverage": "codecov",
    "commit": "commit",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "final-form": "^4.11.1",
    "react": "^16.8.1",
    "react-final-form": "^4.0.2",
    "styled-components": "^4.1.3"
